{
  "targetDefaults": {
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "verify": {
      "dependsOn": [
        "build"
      ]
    },
    "verify:fix": {
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        {
            "projects": "@captainpants/rollup-plugin-sweeter",
            "target": "build"
        }
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default"
    ]
  }
}
