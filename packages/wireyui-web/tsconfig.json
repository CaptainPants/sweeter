    "compilerOptions": {
        "rootDir": "src",
        "jsxImportSource": "-specify-inline-in-each-file-",
        "jsx": "react-jsx",
        "outDir": "./build/esm",
        "paths": {
			"@jsx-test/jsx-runtime": ["./jsx-runtime.js"]
		}
    }
}
